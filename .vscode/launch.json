  "version": "0.2.0",
  "configurations": [
    {
      "name": "article-svc",
      "type": "go",
      "request": "attach",
      "mode": "remote",
      "debugAdapter": "dlv-dap",
      "substitutePath": [
        {
          "from": "${workspaceFolder}/article-svc",
          "to": "/app"
        }
      ],
      "port": 2345,
      "host": "127.0.0.1"
    },
    {
      "name": "user-mgmt-svc",
      "type": "go",
      "request": "attach",
      "mode": "remote",
      "debugAdapter": "dlv-dap",
      "substitutePath": [
        {
          "from": "${workspaceFolder}/user-mgmt-svc",
          "to": "/app"
        }
      ],
      "port": 2346,
      "host": "127.0.0.1"
    }
  ]
}
